{
  "expo": {
    "name": "TOPO",
    "slug": "topo",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "icon": "./assets/fixed-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/TOPO_logo_Full.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#23242f"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ryester.topo",
      "buildNumber": "3",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/fixed-icon.png",
        "backgroundColor": "#23242f"
      },
      "package": "com.ryester.topo"
    },
    "extra": {
      "eas": {
        "projectId": "baa2a43e-a568-4f77-96d3-23750b05d5bf"
      }
    },
    "owner": "ryester",
    "plugins": [
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/fixed-icon.png",
          "color": "#ffffff"
        }
      ]
    ]
  }
}